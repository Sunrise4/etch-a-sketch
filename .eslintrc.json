{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es2020": true
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-plusplus": "off",
    "no-alert": "off"
  }
}
